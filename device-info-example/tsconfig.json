{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "noEmitHelpers": false,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "lib": ["es2017", "DOM"],
    "baseUrl": ".",
    "noErrorTruncation": true,
    "types": ["@nativescript-dom/core-types", "@nativescript-dom/vue-types"]
  },
  "exclude": ["node_modules"]
}
